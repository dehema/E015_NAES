QComboBox#comboxBox_common QAbstractItemView {
    font-size: 14px;
    height:20px;
    outline: 0px solid gray;
    border: 1px solid #298DFF;
    width:120px;
    color: #298DFF;
    background-color: #FFFFFF;
    selection-color:#FFFFFF;
    selection-background-color: #298DFF;
}

QComboBox#comboxBox_common QAbstractItemView::item {
     padding-left: 10px;
     height:28px;
}
/* 下拉框样式 */
QComboBox#comboxBox_common::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 30px;
    height: 30px;
    border-left-width: 0px;
    border-left-color: darkgray;
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}
/* 下拉箭头样式 */
QComboBox#comboxBox_common::down-arrow {
    width: 18px;
    height: 18px;
    color: rgb(158, 158, 158);
    background: transparent;
    padding: 4px 5px 0px 0px;
    image:url(img/arrow_down.png);
}
/* 点击下拉箭头 */
QComboBox#comboxBox_common::down-arrow:on {
    image:url(img/arrow_up.png);
}

