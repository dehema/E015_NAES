QPushButton#bt_common {
    border: 1px solid #298DFF; /* QPushButton边框的宽度、样式和颜色 */
	border-radius: 10px; /* 边框圆角 */
	background-color: #298DFF; /* 背景颜色 */
	color: #FFFFFF; /* 文本颜色 */
}

QPushButton#bt_common:hover{/* 鼠标悬浮在QPushButton上时的状态 */
    background-color: #FFFFFF;
	color: #298DFF;
}

QPushButton#bt_common:pressed { /* 鼠标按压在QPushButton上时的状态 */
    background-color: #F2F2F2;
	color: #298DFF;
}

QPushButton#bt_common:checked { /* 鼠标按压在QPushButton上时的状态 */
	color: #298DFF;
	background-color: #F2F2F2;
}

QPushButton#bt_common:checked:pressed { /* 鼠标按压在QPushButton上时的状态 */
	background-color: #298DFF;
    color: #FFFFFF
}

QPushButton#bt_common:disabled { /* QPushButton禁用时的状态 */
	border: 1px solid #B4B4B4;
	background-color: #FFFFFF;
	color: #B4B4B4;
}

